
<section class="top-ten">
  <h3>Staff Picks!</h3>
  <ul>
    <li>


      <% @staff_picks.each do |p| %>
      <article>
        <img src= "<%= p.photo_url %>" alt="product image">
        <p><%= link_to p.name.capitalize %></p>
        <p>$<%= format_price(p.price) %></p>
      </article>
      <%end%>
    </li>
  </ul>
</section>
