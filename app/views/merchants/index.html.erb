<section class="all-merchants">
  <% @merchants.each do |merchant| %>
  <h2>
    <%= link_to merchant.username, merchant_path(merchant) %>
  </h2>
  <ul class="merchant-display">
    <% merchant.show_four.each do |pro| %>
    <li>
      <h3 class="pro-name"><%= link_to pro.name, product_path(pro) %></h3>
      <%= image_tag pro.image, alt: "bonbon-image", class: "small-pro" %>
    </li>
    <% end %>
  </ul>
  <% end %>
</section>
<%= will_paginate @merchants %>
