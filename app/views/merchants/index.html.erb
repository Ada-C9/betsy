<h1>Merchants</h1>

<section>
  <% @merchants.each do |merchant| %>
  <div class="merch-container">
    <a href="https://placeholder.com"><img src="http://via.placeholder.com/120x120" alt="merchant pic"></a>
    <p>
      <strong>Username: </strong><%= link_to merchant.username, merchant_path(merchant.id) %>
    </p>
    <p>
      <strong>Email: </strong><%= merchant.email %>
    </p>
  </div>
  <% end %>
</section>
