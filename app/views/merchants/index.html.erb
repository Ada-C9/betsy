<h3 class="merchant">Merchants</h3>

<section class="merchants">
  <% @merchants.each do |merchant| %>
  <div class="merchant-card" style="width: 300px;">
    <div class="merchant-content">
      <h5><%= link_to merchant.username, merchant_path(merchant) %></h5>
    </div>
  </div>
  <% end %>
</section>
